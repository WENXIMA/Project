        <activity android:name="com.example.login.InstructorUpdateCourseDetails" />
        <activity android:name="com.example.login.InstructorCourseManage" />
        <activity android:name="com.example.login.InstructorViewStudents" />
        <activity android:name="com.example.login.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.login.Register" />
    </application>

</manifest>